<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="EmployeeRecruitment">

    <insert id="insert" parameterClass="employeeRecruitment">
        INSERT INTO [dbo].[T_OMS_EMPLOYEE_RECRUITMENT]
        ([userCode]
        ,[comeFrom]
        ,[recommendUser])
        VALUES
        (#userCode#
        ,#comeFrom#
        ,#recommendUser#
        )
    </insert>

</sqlMap>