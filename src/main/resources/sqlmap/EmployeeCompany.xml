<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
            PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
            "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="EmployeeCompany">
	<!-- 添加组织部门并返回id -->
	<insert id="insert" parameterClass="employeeCompany">
		INSERT INTO [dbo].[T_OMS_EMPLOYEE_COMPANY]
		(
         [userCode]
		,[companyId]
		)
		VALUES
		(
		#userCode#,
		#companyId#
		)
	</insert>

</sqlMap>
