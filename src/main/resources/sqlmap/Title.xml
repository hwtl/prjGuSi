<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
            PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
            "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Title">

    <select id="query" resultClass="title">
        select id,serialId, titleDegree,titleName from T_OMS_TITLE where status = 1 order by titleDegree
    </select>
    
</sqlMap>